[program:elasticsearch]
command = /scs/usr/elasticsearch/bin/elasticsearch -f -Des.config=/scs/etc/elasticsearch/elasticsearch.yaml -Des.pidfile=/scs/var/run/elasticsearch/elasticsearch.pid
stdout_logfile = /scs/var/log/supervisord/%(program_name)s-stdout.log
stderr_logfile = /scs/var/log/supervisord/%(program_name)s-stderr.log
stopwaitsecs = 30
autorestart = true
